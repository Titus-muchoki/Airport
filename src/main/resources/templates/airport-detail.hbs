{{#partial "content"}}

    <p>Airport:{{ airport.name }} {{airport.code}}{{airport.city}}{{airport.distance}}{{airport.activityId}}</p>
    <p>Activity for this airport: {{ activity. inspectionDate}}{{activity. inspectionArea}}{{activity. inspectionOutcome}}{{activity. serviceAbilityStatus}}</p>
    <h5>Created At:</h5>
    <!--    <p> {{ booking.createdAt }}</p>-->
    <h5>Product status:
        {{#if product.published }}
            <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Completed
        {{else}}
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Open
        {{/if}}
    </h5>

    <p>
        <a href="/airports/{{airport.id}}/edit">Edit tis airport</a><br>
<!--        <a href="/activities/{{airport.activityId}}/airports/{{booking.id}}/delete">DELETE this airport</a>-->
    </p>

{{/partial}}

